{
	"name": "PropertyOccupancyDataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "OccupanyExcelData",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "dimUnitInfo",
						"type": "DatasetReference"
					},
					"name": "GetUnitInfo"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "OccupanyStgTable",
						"type": "DatasetReference"
					},
					"name": "LoadPropertyOccupancyStage"
				}
			],
			"transformations": [
				{
					"name": "split1"
				},
				{
					"name": "lookup1"
				},
				{
					"name": "select1"
				},
				{
					"name": "split2"
				},
				{
					"name": "select2"
				},
				{
					"name": "union1"
				},
				{
					"name": "select3"
				},
				{
					"name": "ErrorReason"
				},
				{
					"name": "ErrorReason2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          UnitIndex as string,",
				"          PropertyIndex as string,",
				"          UnitName as string,",
				"          TenantCode as string,",
				"          TenantName as string,",
				"          OccupancyStartDate as string,",
				"          OccupancyEndDate as string,",
				"          MonthlyRent as string,",
				"          OccupancyStatus as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source(output(",
				"          unit_id as integer,",
				"          unit_index as string,",
				"          property_id as integer,",
				"          unit_name as string,",
				"          unit_type as string,",
				"          number_of_bedrooms as integer,",
				"          number_of_bathrooms as integer,",
				"          unit_area as integer,",
				"          secuirty_deposit as integer,",
				"          monthly_rent as integer,",
				"          unit_status as string,",
				"          lease_start_date as timestamp,",
				"          lease_end_date as timestamp,",
				"          last_updated_at as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> GetUnitInfo",
				"source1 split((isNull(UnitIndex) && isNull(PropertyIndex) && isNull(TenantCode) && isNull(TenantName) && isNull(OccupancyStartDate) && isNull(OccupancyStatus) && isDate(OccupancyStartDate, 'DD-MMM-YYYY') )==false(),",
				"     disjoint: false) ~> split1@(valid, invalid)",
				"split1@valid, GetUnitInfo lookup(UnitIndex == unit_index,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"lookup1 select(mapColumn(",
				"          UnitIndex,",
				"          PropertyIndex,",
				"          UnitName,",
				"          TenantCode,",
				"          TenantName,",
				"          OccupancyStartDate,",
				"          OccupancyEndDate,",
				"          MonthlyRent,",
				"          OccupancyStatus,",
				"          unit_id",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 split(isNull(unit_id)==false(),",
				"     disjoint: false) ~> split2@(validUnit, invalidUnit)",
				"split2@validUnit select(mapColumn(",
				"          UnitIndex,",
				"          PropertyIndex,",
				"          UnitName,",
				"          TenantCode,",
				"          TenantName,",
				"          OccupancyStartDate,",
				"          OccupancyEndDate,",
				"          MonthlyRent,",
				"          OccupancyStatus",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"ErrorReason2, GetUnitInfo union(byName: true)~> union1",
				"union1 select(mapColumn(",
				"          UnitIndex,",
				"          PropertyIndex,",
				"          UnitName,",
				"          TenantCode,",
				"          TenantName,",
				"          OccupancyStartDate,",
				"          OccupancyEndDate,",
				"          MonthlyRent,",
				"          OccupancyStatus,",
				"          error_reason",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select3",
				"split2@invalidUnit derive(error_reason = 'Invalid Unit Index Value') ~> ErrorReason",
				"split1@invalid derive(error_reason = 'Mandatory columns must not empty') ~> ErrorReason2",
				"select2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     recreate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> LoadPropertyOccupancyStage"
			]
		}
	}
}